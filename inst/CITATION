citHeader("To cite package 'BayesBridge' in publications use:")

citEntry(entry  = "Manual",
	 title  = "Package 'BayesBridge'",
	 author = personList(as.person("Nicholas G. Polson"),
		             as.person("James G. Scott"),
			     as.person("Jesse Windle"  )),
         year = 2012,
	 textVersion = 
	   paste("Nicholas G. Polson, James G. Scott, and Jesse Windle (2012).",
          "Package 'BayesBridge.'",
          "URL: http://cran.r-project.org/web/packages/.")
)
